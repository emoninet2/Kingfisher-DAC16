#ifndef INC_CMDPARSER_H_
#define INC_CMDPARSER_H_

#include "main.h"


void initCmd();




typedef enum{
	CMD_DAC_NOP = 0,
	CMD_GET_DAC_DEVICEID,
	CMD_GET_DAC_STATUS,
	CMD_GET_DAC_SPICONFIG,
	CMD_SET_DAC_SPICONFIG,
	CMD_GET_DAC_GENCONFIG,
	CMD_SET_DAC_GENCONFIG,
	CMD_GET_DAC_BRDCSTCONFIG,
	CMD_SET_DAC_BRDCSTCONFIG,
	CMD_GET_DAC_SYNCCONFIG,
	CMD_SET_DAC_SYNCCONFIG,
	CMD_GET_DAC_TOGGLECONFIG,
	CMD_SET_DAC_TOGGLECONFIG,
	CMD_GET_DAC_POWERDOWN,
	CMD_SET_DAC_POWERDOWN,
	CMD_SET_DAC_RANGE,
	CMD_SET_DAC_TRIGGER,
	CMD_GET_DAC_BRDCSTREG,
	CMD_SET_DAC_BRDCSTREG,
	CMD_GET_DAC_DACnREG,
	CMD_SET_DAC_DACnREG,
	CMD_GET_DAC_OFFSET,
	CMD_SET_DAC_OFFSET,
}cmd_t;



void parseCmd(uint8_t *data, uint32_t len);


#endif /* INC_CMDPARSER_H_ */
